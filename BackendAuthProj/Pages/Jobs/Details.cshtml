@page "{jobId:int}"
@model BackendAuthProj.Pages.Jobs.DetailsModel
@{
    ViewData["Title"] = "Details";
}

<h1>@Model.Job.Title</h1>
<div>
    Id: @Model.Job.Id
</div>
<div>
    Title: @Model.Job.Title
</div>

<div>
    Description: @Model.Job.Description
</div>
<div>
    Expiration Date: @Model.Job.ExpirationDate
</div>

<a asp-page="./List" class="btn btn-default">See All Jobs</a>